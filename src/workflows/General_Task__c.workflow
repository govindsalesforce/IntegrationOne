    <rules>
        <fullName>Action Plan has been assigned for Doc number</fullName>
        <actions>
            <name>Send_email_to_Action_Plan_owner_once_Action_plan_is_assigned</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>General_Task__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>DocumentChange</value>
        </criteriaItems>
        <criteriaItems>
            <field>General_Task__c.Status__c</field>
            <operation>equals</operation>
            <value>Pending ActionPlan</value>
        </criteriaItems>
        <description>Email should be sent to the Action plan owner once action plan for doc is created and assigned in cr.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
