    <rules>
        <fullName>Recall with out Approval For Implementation</fullName>
        <active>true</active>
        <description>Recall with out Approval For Implementation</description>
        <formula>AND(NOT(ISNULL( CAPA_ID__c )), 
ISBLANK(Non_Conformance__c) ,ISPICKVAL(isSubmitted__c,&apos;False&apos;),ISPICKVAL( Status__c ,&apos;Open&apos;),( RecordType.DeveloperName==&apos;Implementation&apos;))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
