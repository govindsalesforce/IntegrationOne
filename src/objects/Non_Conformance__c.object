    <validationRules>
        <fullName>Defect_Statement_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>Full_Description__c = &apos;&apos;</errorConditionFormula>
        <errorMessage>Defect Statement is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defect_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Defect__c)</errorConditionFormula>
        <errorDisplayField>Defect__c</errorDisplayField>
        <errorMessage>Defect is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NC_Source_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(NC_Source__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>NC_Source__c</errorDisplayField>
        <errorMessage>NC Source is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NC_Type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL( NC_Type__c , &quot;--None--&quot;),ISPICKVAL( NC_Type__c , &quot;&quot;))</errorConditionFormula>
        <errorMessage>NC Type is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Occurance_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Occurance_Date__c)</errorConditionFormula>
        <errorDisplayField>Occurance_Date__c</errorDisplayField>
        <errorMessage>Occurance Date Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Occurance_Date_not_future</fullName>
        <active>true</active>
        <errorConditionFormula>Occurance_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Occurance_Date__c</errorDisplayField>
        <errorMessage>Occurrence Date cannot be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Occurence_Date_greater_than_Reported</fullName>
        <active>true</active>
        <description>Occurrence Date cannot be greater than the Reported Date</description>
        <errorConditionFormula>Occurance_Date__c &gt;  Reported_Date__c</errorConditionFormula>
        <errorDisplayField>Occurance_Date__c</errorDisplayField>
        <errorMessage>Occurrence Date cannot be greater than the Reported Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Source_is_required</fullName>
        <active>true</active>
        <description>&quot;Other Source&quot; field is required if NC Source is &quot;Other&quot;</description>
        <errorConditionFormula>ISPICKVAL( NC_Source__c , &quot;Other&quot;) &amp;&amp; ISBLANK(Other_Source__c)</errorConditionFormula>
        <errorDisplayField>Other_Source__c</errorDisplayField>
        <errorMessage>Other Source field is required because NC Source value is Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Process_is_required</fullName>
        <active>true</active>
        <description>Process is required if NC type is selected as &quot;Process&quot;.</description>
        <errorConditionFormula>ISPICKVAL( NC_Type__c , &quot;Process&quot;) &amp;&amp;  ISBLANK(Process__c)</errorConditionFormula>
        <errorMessage>Process is required as NC type is Process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_is_required</fullName>
        <active>true</active>
        <description>Product is required if NC type is selected as &quot;Product&quot;.</description>
        <errorConditionFormula>ISPICKVAL( NC_Type__c , &quot;Product&quot;) &amp;&amp;  ISBLANK(Product__c)</errorConditionFormula>
        <errorMessage>Product is required as NC type is product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reported_By_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Reporter__c)</errorConditionFormula>
        <errorMessage>Reporter Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reported_Date_Not_Future</fullName>
        <active>true</active>
        <errorConditionFormula>Reported_Date__c &gt; Today()</errorConditionFormula>
        <errorDisplayField>Reported_Date__c</errorDisplayField>
        <errorMessage>Reported Date cannot be a future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reported_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Reported_Date__c)</errorConditionFormula>
        <errorDisplayField>Reported_Date__c</errorDisplayField>
        <errorMessage>Reported Date Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reported_Date_cannot_be_before_Occurance</fullName>
        <active>true</active>
        <errorConditionFormula>Reported_Date__c &lt; Occurance_Date__c</errorConditionFormula>
        <errorDisplayField>Reported_Date__c</errorDisplayField>
        <errorMessage>Reported Date cannot be less than Occurrence Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Risk_Description_required</fullName>
        <active>false</active>
        <errorConditionFormula>Risk_Description__c=&quot;&quot;</errorConditionFormula>
        <errorDisplayField>Risk_Description__c</errorDisplayField>
        <errorMessage>Risk Description is required.</errorMessage>
    </validationRules>
