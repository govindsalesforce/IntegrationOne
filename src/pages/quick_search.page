 function searchDoc()
 {
    var PrereleaseVal;
    var CurrentVal;
    var bookmark;
    if($('#PreReleaseCheck').is(":checked"))
        PrereleaseVal = 'Prerelease';
    else
        PrereleaseVal = '';
    if($('#currentCheck').is(":checked"))
        CurrentVal = 'Current';
     else
        CurrentVal = '';
    if($('#bookmarkedCheck').is(":checked"))
        bookmark = 'true';
     else
        bookmark = '';

    showloading();
    searchServerDoc(
        PrereleaseVal,
        CurrentVal,
        bookmark,
        document.getElementById("sObjectID").options[document.getElementById("sObjectID").selectedIndex].value
    );
 }

 
 function searchNC()
 {
    var openNC;
    var workflowNC;
    if($('#OpenNCCheck').is(":checked"))
        openNC = 'Open';
    else
        openNC = '';
    if($('#workflowNCValue').is(":checked"))
        workflowNC = 'NC Workflow';
     else
        workflowNC = '';
    showloading();
    searchServerNC(
        openNC,
        workflowNC,
        document.getElementById("sObjectID").options[document.getElementById("sObjectID").selectedIndex].value
    );
 }

 function searchCAPA()
 {
    var openCAPA;
    if($('#OpenCapaCheck').is(":checked"))
        openCAPA = 'Open';
    else
        openCAPA = '';
    showloading();
    searchServerCAPA(
        openCAPA,
        document.getElementById("sObjectID").options[document.getElementById("sObjectID").selectedIndex].value
    );
 }

